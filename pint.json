{
    "preset": "laravel",
    "rules": {
        "concat_space": {
            "spacing": "one"
        },
        "function_declaration": {
            "closure_function_spacing": "one"
        },
        "single_quote": {
            "strings_containing_single_quote_chars": true
        },
        "whitespace_after_comma_in_array": true
    }
}